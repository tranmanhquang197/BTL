  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "postinstall": "ngcc",
    "build:deleteOldFiles": "if exist \"D:/Product_origin/Mango-build/\" rmdir /s /q \"D:/Product_origin/Mango-build/Mango/\"",
    "build:copyNewFiles": "xcopy \"dist/Mango\" \"D:/Product_origin/Mango-build/Mango\" /s/h/e/k/f/c/i",
    "build:69": "npm run build:deleteOldFiles && ng build --configuration production && npm run build:copyNewFiles",
    "build:cloud": "npm run build:deleteOldFiles && ng build --configuration cloud && npm run build:copyNewFiles",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "delete:lib:built": "if exist \"./../ac_base/NextSolutionsBase_lib\" rmdir /s /q \"./../ac_base/NextSolutionsBase_lib\"",
    "delete:lib:install": "if exist \"node_modules/@next-solutions/next-solutions-base\" rmdir /s /q \"node_modules/@next-solutions/next-solutions-base\"",
    "delete:lib": "npm run delete:lib:built && npm run delete:lib:install",
    "ng-packagr": "ng-packagr -p \"./../ac_base/NextSolutionsBase_web/ng-packagr.json\"",
    "build:lib:copyNewFiles": "xcopy \"./../ac_base/NextSolutionsBase_lib\" \"node_modules/@next-solutions/next-solutions-base\" /s/h/e/k/f/c/i",
    "install:local": "npm install \"./../ac_base/NextSolutionsBase_lib\" --save",
    "build:lib": "npm run delete:lib && npm run ng-packagr && npm run install:local",
    "delete:lib:built-linux": "if [ -d \"./../ac_base/NextSolutionsBase_lib\", ]; then sudo rm -rf \"./../ac_base/NextSolutionsBase_lib\",; fi",
    "delete:lib:install-linux": "if [ -d \"node_modules/@next-solutions/next-solutions-base\", ]; then sudo rm -rf \"node_modules/@next-solutions/next-solutions-base\",; fi",
    "delete:lib-linux": "npm run delete:lib:built-linux && npm run delete:lib:install-linux",
    "build:lib-linux": "npm run delete:lib-linux && npm run ng-packagr && npm run install:local"
  },
